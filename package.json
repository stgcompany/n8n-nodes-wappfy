{
  "name": "@wappfy/n8n-nodes-wappfy",
  "version": "0.1.2",
  "description": "n8n nodes for Wappfy WhatsApp API integration",
  "keywords": [
    "n8n-community-node-package",
    "whatsapp",
    "wappfy",
    "api",
    "messaging"
  ],
  "license": "MIT",
  "homepage": "https://github.com/stgcompany/n8n-nodes-wappfy",
  "author": {
    "name": "Wappfy",
    "email": "suporte@wappfy.com.br"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stgcompany/n8n-nodes-wappfy.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WappfyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Wappfy/Wappfy.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}